{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@core/*": [
        "src/core/*"
      ],
      "@domains/*": [
        "src/domains/*"
      ],
      "@constants/*": [
        "src/_constants/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/types/global.d.ts"
  ]
}